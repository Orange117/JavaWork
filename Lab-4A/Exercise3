
public class Exercise3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Fahrenheit\tClsius");
		System.out.println("======================");
		for(int i = 0;i <= 20;i++) {
			double t = Celsius(i);
			System.out.println(i + "\t\t" + String.format("%.2f", t));
		}
	}
	
	public static double Celsius(double F) {
		return ((5 * 1.00)/(9 * 1.00) * (F-32));
	}
}
